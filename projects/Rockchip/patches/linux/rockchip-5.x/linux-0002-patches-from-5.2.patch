 	pinctrl: pinctrl {
 		compatible = "rockchip,rk3288-pinctrl";
 		rockchip,grf = <&grf>;

From a094f6cc591ad82e0ada443df1c64490a3b46257 Mon Sep 17 00:00:00 2001
